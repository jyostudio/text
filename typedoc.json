{
    "plugin": [
        "typedoc-github-theme"
    ],
    "entryPoints": [
        "./src/index.ts"
    ],
    "exclude": [
        "**/*.test.ts",
        "**/node_modules/**"
    ],
    "out": "./docs",
    "lang": "zh",
    "hideGenerator": true,
    "disableSources": true,
    "tsconfig": "tsconfig.json",
    "suppressCommentWarningsInDeclarationFiles": true,
    "skipErrorChecking": true
}